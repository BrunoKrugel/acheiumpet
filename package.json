{
	"name": "musee",
	"type": "module",
	"version": "1.14.2",
	"private": true,
	"sideEffects": false,
	"author": "Ayaan Zaidi (hi@obviy.us)",
	"license": "GNU GPLv3",
	"description": "Self hosted gallery written in React + Astro. Ships minimal JS to the client.",
	"scripts": {
		"predev": "npx tsx imageImporter.ts",
		"dev": "astro dev",
		"start": "npm run dev",
		"prebuild": "npx tsx imageImporter.ts",
		"build": "astro build",
		"postbuild": "npx @divriots/jampack ./dist",
		"preview": "astro preview",
		"astro": "astro"
	},
	"dependencies": {
		"@astrojs/prefetch": "^0.4.1",
		"@astrojs/react": "^3.3.0",
		"@astrojs/tailwind": "^5.1.0",
		"@types/react": "^18.2.77",
		"@types/react-dom": "^18.2.25",
		"astro": "4.16.6",
		"astro-critters": "^2.1.6",
		"astro-purgecss": "^4.1.0",
		"exifreader": "^4.22.1",
		"fecha": "^4.2.3",
		"framer-motion": "^11.0.28",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-masonry-css": "^1.0.16",
		"sharp": "^0.33.3",
		"sqids": "^0.3.0",
		"tailwindcss": "^3.4.3"
	},
	"repository": "git@github.com:obviyus/musee.git",
	"devDependencies": {
		"@biomejs/biome": "^1.6.4",
		"@types/node": "^18.18.5",
		"tsx": "^4.7.2"
	},
	"release": {
		"branches": [
			"master"
		],
		"plugins": [
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/npm",
			"@semantic-release/git",
			"@semantic-release/github"
		]
	}
